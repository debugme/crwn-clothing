{
  "name": "crwn-clothing",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "14.15.1"
  },
  "dependencies": {
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "4.0.1",
    "typescript": "4.1.3"
  },
  "devDependencies": {
    "@types/jest": "26.0.20",
    "@types/node": "12.19.13",
    "@types/node-sass": "4.11.1",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-router-dom": "5.1.7",
    "@types/rimraf": "3.0.0",
    "node-sass": "4.14.1",
    "rimraf": "3.0.2",
    "typac": "0.1.10"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "docker:build:image": "docker build . -f Dockerfile -t debugme/crwn-clothing",
    "docker:run:container": "docker run -it -p 3000:3000 -v $(pwd)/src:/app/src debugme/crwn-clothing",
    "docker:run:shell": "docker run -it debugme/crwn-clothing sh",
    "docker:connect:shell": "docker exec -it `docker ps | grep crwn-clothing | awk '{ print $1}'` sh",
    "clean:install": "rimraf node_modules && npm install",
    "deps": "typac",
    "devs": "typac --dev"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
